{
  "buildCommand": "cd frontend/dashboard && npm install && npm run build",
  "installCommand": "npm install",
  "outputDirectory": "frontend/dashboard/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
